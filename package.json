{
  "name": "@prisme.ai/pyramide",
  "version": "24.05.231411",
  "description": "Prisme.ai event driven automation platform",
  "workspaces": [
    "./services/*",
    "./packages/*"
  ],
  "scripts": {
    "start": "ts-node scripts/start-dev.ts",
    "clean": "npx rimraf packages/**/dist",
    "build:console": "npm run build --prefix services/console",
    "build:pages": "npm run build --prefix services/pages",
    "build:workspaces": "npm run build --prefix services/workspaces",
    "build:gateway": "npm run build --prefix services/api-gateway",
    "build:runtime": "npm run build --prefix services/runtime",
    "build:events": "npm run build --prefix services/events",
    "build:validation:instruction": "ts-node scripts/buildInstructions.ts specifications/swagger.yml packages/validation/instructions.json",
    "build:validation": "npm run build --prefix ./packages/validation && npm run build:validation:instruction",
    "build:design-system": "npm run build --prefix ./packages/design-system",
    "build:types": "npx dtsgen --out ./packages/types/index.d.ts ./specifications/swagger.yml",
    "build:packages": "npm run build:types && npm run build:validation:instruction && npx rollup -c",
    "dev:console": "npm run dev --prefix services/console",
    "dev:pages": "PORT=3100 npm run dev --prefix services/pages",
    "dev:front": "concurrently \"npm run dev:console\" \"npm run dev:pages\"",
    "dev:design-system": "npm start --prefix packages/design-system",
    "dev:workspaces": "npm run dev --prefix services/workspaces",
    "dev:api-gateway": "npm run dev --prefix services/api-gateway",
    "dev:runtime": "npm run dev --prefix services/runtime",
    "dev:events": "npm run dev --prefix services/events",
    "dev:docs": "npm run dev --prefix docs",
    "publish:types": "npm publish --workspace=@prisme.ai/types --access=public",
    "publish:broker": "npm publish --workspace=@prisme.ai/broker --access=public",
    "publish:design-system": "npm publish --workspace=@prisme.ai/design-system --access=public",
    "publish:blocks": "npm publish --workspace=@prisme.ai/blocks --access=public",
    "publish:sdk": "npm publish --workspace=@prisme.ai/sdk --access=public",
    "check:console": "tsc -p services/console/tsconfig.json",
    "check:pages": "tsc -p services/pages/tsconfig.json",
    "check:translations:console": "npx ts-node scripts/checkTranslations.ts services/console/public/locales en",
    "check:translations:pages": "npx ts-node scripts/checkTranslations.ts services/pages/public/locales en",
    "check:front": "npm run check:translations:console && npm run check:translations:pages && npm run check:console && npm run check:pages && npm test -- packages/blocks -u && npm test -- packages/design-system -u && npm test -- services/console -u && npm test -- services/pages -u",
    "next:tag": "ts-node ./scripts/nexttag.ts | pbcopy",
    "test": "jest --collect-coverage",
    "test-e2e": "npx playwright test",
    "deploy": "ts-node ./scripts/deploy.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/gogowego-devsecops/dev/pyramide.git"
  },
  "author": "Prisme.ai",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/gogowego-devsecops/dev/pyramide/issues"
  },
  "homepage": "https://gitlab.com/gogowego-devsecops/dev/pyramide#readme",
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.15.0",
    "@dtsgenerator/replace-namespace": "^1.5.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@svgr/webpack": "^6.2.1",
    "@types/ace": "^0.0.47",
    "@types/color": "^3.0.3",
    "@types/css": "^0.0.33",
    "@types/hash-sum": "^1.0.0",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^3.0.1",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash.get": "^4.4.7",
    "@types/mustache": "^4.2.2",
    "@types/node-fetch": "^2.5.12",
    "@types/pretty": "^2.0.1",
    "@types/react": "^17.0.24",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.0",
    "@types/react-final-form-listeners": "^1.0.0",
    "@types/react-native": "^0.66.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/shelljs": "^0.8.11",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "^0.17.5",
    "concurrently": "^7.0.0",
    "dtsgenerator": "^3.13.2",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.2.2",
    "less-watch-compiler": "^1.16.3",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^7.4.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript2": "^0.31.0",
    "sass": "^1.42.1",
    "shelljs": "^0.8.5",
    "shelljs-live": "^0.0.5",
    "storybook-addon-react-docgen": "^1.2.42",
    "storybook-preset-less": "^1.1.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },

  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@playwright/test": "^1.43.0",
    "@types/marked": "^4.0.8",
    "antd": "^4.23.0",
    "hash-sum": "^2.0.0",
    "patch-package": "^6.4.7"
  }
}
